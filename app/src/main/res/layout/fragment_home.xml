<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D0D3D4"
    android:orientation="vertical"
    android:padding="2dp"
    tools:context=".fragment_home.HomeFragment"
   >
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#D0D3D4"
        android:orientation="vertical"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:weightSum="2"
                android:layout_margin="10dp"
                android:elevation="5dp"
                android:padding="10dp"
                android:background="@drawable/frangmentlayout"
                >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"

                    >
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/worldrp"
                        android:gravity="center"
                        android:elevation="5dp"
                        android:clickable="true"
                        android:id="@+id/txtworld"
                        android:focusable="true" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="World"
                        android:textColor="#505050"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="10dp"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/indiarp"
                        android:id="@+id/txtIndia"
                        android:textColor="@color/colorwhite"
                        android:textSize="25sp"
                        android:textStyle="bold"
                        android:gravity="center"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="India"
                        android:textColor="#505050"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:weightSum="2"
                android:layout_margin="10dp"
                android:elevation="5dp"
                android:padding="10dp"
                android:background="@drawable/frangmentlayout"
                >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"

                    >
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/enviournmentrp"
                        android:gravity="center"
                        android:elevation="5dp"
                        android:padding="5dp"

                        android:id="@+id/txtEnviournment"
                        android:textColor="@color/colorwhite"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Environment"
                        android:textSize="18sp"
                        android:textColor="#505050"
                        android:textStyle="bold"
                        />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:gravity="center_horizontal">
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/artcultutrrp"
                        android:padding="5dp"
                        android:id="@+id/txtartandculture"
                        android:textColor="@color/colorwhite"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:gravity="center"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Art and Culture"
                        android:textSize="18sp"
                        android:textColor="#505050"
                        android:textStyle="bold"
                        />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:weightSum="2"
                android:layout_margin="10dp"
                android:elevation="5dp"
                android:padding="10dp"
                android:background="@drawable/frangmentlayout"
                >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center_horizontal"

                    >
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/spacerp"
                        android:gravity="center"
                        android:elevation="5dp"
                        android:padding="5dp"
                        android:id="@+id/txtspace"
                        android:textColor="@color/colorwhite"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Space"
                        android:textColor="#505050"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:gravity="center_horizontal">
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/defencerp"
                        android:padding="5dp"
                        android:id="@+id/txtdefence"
                        android:textColor="@color/colorwhite"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:gravity="center"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Defence"
                        android:textColor="#505050"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:weightSum="2"
                android:layout_margin="10dp"
                android:elevation="5dp"
                android:padding="10dp"
                android:background="@drawable/frangmentlayout"
                >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center_horizontal"

                    >
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/historyrp"
                        android:gravity="center"
                        android:elevation="5dp"
                        android:padding="5dp"
                        android:id="@+id/txthistory"
                        android:textColor="@color/colorwhite"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="History"
                        android:textColor="#505050"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:gravity="center_horizontal">
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/generalsciencerp"
                        android:padding="5dp"
                        android:id="@+id/txtgeneralscience"
                        android:textColor="@color/colorwhite"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:gravity="center"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="General Science"
                        android:textSize="18sp"
                        android:textColor="#505050"
                        android:textStyle="bold"
                        />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:weightSum="2"
                android:layout_margin="10dp"
                android:elevation="5dp"
                android:padding="10dp"
                android:background="@drawable/frangmentlayout"
                >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center_horizontal"

                    >
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/historyrp"
                        android:gravity="center"
                        android:elevation="5dp"
                        android:padding="5dp"
                        android:id="@+id/txtphysics"
                        android:textColor="@color/colorwhite"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Physics"
                        android:textColor="#505050"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:gravity="center_horizontal">
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/generalsciencerp"
                        android:padding="5dp"
                        android:id="@+id/txtchemistry"
                        android:textColor="@color/colorwhite"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:gravity="center"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Chemistry"
                        android:textSize="18sp"
                        android:textColor="#505050"
                        android:textStyle="bold"
                        />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:weightSum="2"
                android:layout_margin="10dp"
                android:elevation="5dp"
                android:padding="10dp"
                android:background="@drawable/frangmentlayout"
                >
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_weight="1"
                    android:orientation="vertical"
                    android:gravity="center_horizontal"

                    >
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/historyrp"
                        android:gravity="center"
                        android:elevation="5dp"
                        android:padding="5dp"
                        android:id="@+id/txtbiology"
                        android:textColor="@color/colorwhite"
                        android:textSize="20sp"
                        android:textStyle="bold"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Biology"
                        android:textColor="#505050"
                        android:textSize="18sp"
                        android:textStyle="bold"
                        />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:gravity="center_horizontal">
                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:background="@drawable/generalsciencerp"
                        android:padding="5dp"
                        android:id="@+id/txtgeneralscienc"
                        android:textColor="@color/colorwhite"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:gravity="center"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="General Science"
                        android:textSize="18sp"
                        android:textColor="#505050"
                        android:textStyle="bold"
                        />
                </LinearLayout>
            </LinearLayout>



        </LinearLayout>
    </ScrollView>
</RelativeLayout>